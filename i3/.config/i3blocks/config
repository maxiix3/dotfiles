[load]
label= 
command=/usr/lib/i3blocks/cpu_usage
interval=5
color=#6c71c4

[temp]
label=
command=echo "$(sensors coretemp-isa-0000 | awk '/Package/ {print $4}')"
interval=10
color=#b58900

[time]
label=
command=date '+%a %d %b %H:%M:%S'
interval=5
color=#50C878
