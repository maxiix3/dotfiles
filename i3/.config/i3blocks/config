[disk-home]
label=:
command=/usr/lib/i3blocks/disk $HOME
color=#859900
interval=60

[ssid]
label=
command=echo "$(iw dev wlp59s0 link | grep SSID | cut -d " " -f 2-)"
color=#00FF00
interval=10
markup=pongo
separator=false

[wifi]
instance=wlp59s0
