create table kunde(
    kundenummer int primary key,
    kundenavn varchar(50),
    kundeadresse varchar(80),
    kundefakturaadresse varchar(80)
);
create table Offentlig_etat(
    kundenummer int primary key references kunde(kundenummer),
    departement varchar(80)
);
create table firma(
    kundenummer int primary key references kunde(kundenummer),
    org_nummer int unique
);
create table gruppe(
    gruppenavn varchar(80) primary key,
    timelønn int
);
create table telefonnummer(
    nummer int primary key,
    kundenummer int references kunde(kundenummer)
);
create table ansatt(
    ansattnr int primary key,
    gruppenavn varchar(80) references gruppe(gruppenavn)
);
create table prosjekt(
    prosjektnummer int primary key,
    prosjektleder int references ansatt(ansattnr),
    prosjektnavn varchar(80),
    kundenummer int references kunde(kundenummer)
);
create table AnsattDeltarIProsjekt(
    ansattnr int references ansatt(ansattnr),
    prosjektnr int references prosjekt(prosjektnummer),
    primary key(ansattnr, prosjektnr)
);

SELECT kundenummer, kundenavn, kundeadresse
FROM kunde
;

SELECT nummer, kundenummer
FROM telefonnummer
ORDER BY kundenummer
;

SELECT DISTINCT prosjektleder
FROM prosjekt
GROUP BY prosjektleder
;

SELECT ansattnr
FROM ansattdeltariprosjekt A, prosjekt P
WHERE P.PROSJEKTNAVN = 'RUTER APP' AND A.PROSJEKTNR = P.PROSJEKTNUMMER
;

SELECT ANSATTNR, TIMELØNN
FROM ANSATT A, GRUPPE G
WHERE A.GRUPPENAVN = G.GRUPPENAVN
;

SELECT org_nummer, kundenavn
FROM firma F, kunde K
WHERE F.kundenummer = K.kundenummer
;
