CC = gcc
CCX = gcc-4.4
CFLAGS = -Wall -g

oblig2:ifish.o safefork.o
	$(CC) $(CFLAGS) ifish.o safefork.o -o oblig2

arch:ifisha.o safeforka.o
	$(CCX) $(CFLAGS) ifish.o safefork.o -o oblig2

arch_debug:ifishad.o safeforka.o
	$(CCX) $(CFLAGS) ifish.o safefork.o -o oblig2

debug:ifishd.o safefork.o
	$(CC) $(CFLAGS) ifish.o safefork.o -o oblig2

ifishd.o:ifish.c
	$(CC) $(CFLAGS) -DDEBUG -c ifish.c

ifishad.o:ifish.c
	$(CCX) $(CFLAGS) -DDEBUG -c ifish.c

ifish.o:ifish.c
	$(CC) $(CFLAGS) -c ifish.c

ifisha.o:ifish.c
	$(CCX) $(CFLAGS) -c ifish.c

safeforka.o:safefork.c
	$(CCX) $(CFLAGS) -c -Darch safefork.c

safefork.o:safefork.c
	$(CC) $(CFLAGS) -c safefork.c

.PHONY:clean
	
clean: 
	rm -f *.o oblig2
