! -- Fonts -- !
URxvt.font:                 xft:Dejavu Sans Mono for Powerline:bold:size=12
XTerm*faceName:             xft:Dejavu Sans Mono for Powerline:bold:size=12


!URxvt.font:                xft:monospace:pixelsize=18
!URxvt.boldfont:            xft:monospace-Bold:pixelsize=18

!Terminus
!URxvt.font:                xft:Terminus:pixelsize=20:style=Regular
!URxvt.boldFont:            xft:Terminus:pixelsize=20:style=Bold

!URxvt.font:                xft:bitstream vera sans mono:size=12:antialias=true
!URxvt.boldFont:            xft:bitstream vera sans mono:bold:size=12:antialias=true

!URxvt*font:                 xft:DejaVu\ Sans\ Mono:bold:pixelsize=15:antialias=true,xft:DejaVu\ Sans\ Mono\ for\ Powerline:bold:pixelsize=15:antialias=true

!DejaVuSans
