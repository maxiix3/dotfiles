! scrollbar style - rxvt(default), plain (most compact), next, or xterm
URxvt.scrollstyle:          rxvt
!URxvt.letterSpace:          -1
URxvt*scrollBar:            false
URxvt*termName:             rxvt-256color
URxvt*TERM:                 xterm-256color
URxvt.urgentOnBell:         True
Xterm*utf8:                 2
URxvt.depth:                0

! -- copy/paste --!
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

! -- perl extensions -- !
!URxvt.perl-ext-common: extension_name_1,extension_name_2

! -- clickable URLs --!
URxvt.perl-ext-common: default,matcher,selection-to-clipboard,resize-font
URxvt.url-launcher: /usr/bin/google-chrome-stable
URxvt.matcher.button: 1

! -- Xft settings -- !
Xft.dpi:                    180
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintfull

!! -- Colors -- !
!URxvt*background:           black
URxvt*background:           #121212
URxvt*foreground:           #FFFFFF
XTerm*background:           #121212
XTerm*foreground:           #FFFFFF
