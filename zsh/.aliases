#Screenfix	
alias hdmi1080='xrandr --output DP3 --scale 2x2'	
alias dockingtb16right='xrandr --output DP1-2 --scale 2x2 --primary --output DP1-3 --pos 3840x0 --scale 2x2'	
alias dockingtb16left='xrandr --output DP1-2 --scale 2x2 --primary --output DP1-3 --pos -3840x0 --scale 2x2'	
alias dockingwd15='xrandr --output DP1-1 --scale 2x2 --primary --output DP1-3 --pos 3840x0 --scale 2x2'	
alias simula='xrandr --output DP1-2 --scale 1.5x1.5 --pos -3840x0'	
