# Alias file	

alias uiodisk='sshfs  maximinh@login.ifi.uio.no: ~/uiodisk -o reconnect,modules=iconv,from_code=utf8,auto_cache'	
alias usb='cd /mnt/usbflash'	
alias USB='mount /mnt/usbflash && cd /mnt/usbflash' 	
alias colemak='setxkbmap us -v colemak'	
alias norsk='setxkbmap no'	
alias uiosock='ssh -D 8123 -f -C -q -N maximinh@login.uio.no'	
alias whatthecommit='curl http://whatthecommit.com/index.txt | git commit --file -'	
alias dear-diary='~/git/dotfiles/scripts/deardiary.sh'	
alias pull-all='~/git/update-git.sh'	

alias android-mount='jmtpfs ~/mnt'	
alias android-unmount='fusermount -u ~/mnt'	

alias capsoff="xmodmap -e 'clear Lock' -e 'keycode 0x42 = Escape'"	
alias capson="xmodmap -e 'clear Lock' -e 'keycode 0x42 = Caps_Lock'"	

# renames	
alias stow='stow -d ~/dotfiles'	
alias cp='rsync -ah --progress'	
alias eclim='/usr/lib/eclipse/eclimd'	
alias bosch='~/build/bochs-2.6.8/bochs -f ~/build/bochs-2.6.8/.bochsrc'	
alias ls='ls -h --color=auto'	
alias vim='nvim'	
alias vi='vim'	
alias mutt='cd ~/Documents/attachments && neomutt'	
alias vimrc='cd ~/.config/nvim/'	
alias enw='enw -i'	

