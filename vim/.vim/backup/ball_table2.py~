y = []  # making a empty list to fill later
t = []  # making a emptu list to fill later
n = 10    # no of subintervals in I
v0 = 10   # start velosity
g = 9.81    # gravity force
dt = ((2*v0)/g)/n    # subinterval size
tt = 0

for i in range(n+1):
    yt = (v0 * tt)-(0.5 * g * tt ** 2)
    t.append(tt)
    y.append(yt)
    tt = tt + dt
dsasds sd
# making a zip and running it in a for loop
for (z, x) in zip(t, y):
    print 'T: %6.3f   Y: %6.3f' % (z, x)

