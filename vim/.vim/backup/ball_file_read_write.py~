z=open
f=float
def r():
    with z("file.txt") as i:
        a = i.read().split()
        return f(a[1]),sorted(map(f,a[3:]))
def p(g):
    with z("out.txt","w") as o:
        map(lambda i:o.write("t = %8.3f y =%8.3f \n" %(g[1][i],[g[0]*a-0.5*9.81*(a**2) for a in g[1]][i])),range(len(g[1])))


p(r())




