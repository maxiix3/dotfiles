CC = gcc
CFLAGS = -g -Wall

all:oppg1

oppg1: main.o print.o len.o random.o vowel.o
	$(CC) $(CFLAGS) main.o print.o len.o random.o vowel.o -o oppg1

main.o:main.c
	$(CC) $(CFLAGS) -c main.c
len.o:len.c
	$(CC) $(CFLAGS) -c len.c
print.o:print.c
	$(CC) $(CFLAGS) -c print.c
random.o:random.c
	$(CC) $(CFLAGS) -c random.c
vowel.o:vowel.c
	$(CC) $(CFLAGS) -c vowel.c

