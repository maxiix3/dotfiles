#include "header.h"
/* main metoden som tar inn argumenter fra terminalen og kaller p√•
 * metoden til den koresponderende kommandoen
 */
int main(int argc, char* argv[]){
    if(argc < 2){
        print_opt();
        return 0;
    }
    if(strcmp(argv[1], "p") == 0){
        if(argc != 3){
            print_opt();
        } else{
            print_fi(argv[2]);
        }
    } else if(strcmp(argv[1], "e") == 0) {
        if (argc != 4){
            print_opt()
        } else{
            encode(argv[2], argv[3]);
        }
    } else if(strcmp(argv[1], "d") == 0){
        if(argc != 3){
            print_opt()
        } else{
            decode(argv[2]);
            printf("\n");
        }
    }else{
        print_opt();
    }
    return 0;
}
